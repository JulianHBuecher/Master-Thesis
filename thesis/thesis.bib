	keywords = {Batteries, Charging infrastructure, conductive and inductive charging, Connectors, Educational institutions, integrated chargers, levels 1, 2, and 3 chargers, plug-in electric vehicles ({PEVs}), plug-in hybrid electric vehicles ({PHEVs}), Safety, Standards, Topology, unidirectional/bidirectional chargers, Vehicles},
	file = {IEEE Xplore Abstract Record:/Users/I552343/Zotero/storage/6GWUAF6X/stamp.html:text/html;IEEE Xplore Full Text PDF:/Users/I552343/Zotero/storage/HJX9HH9C/Yilmaz and Krein - 2013 - Review of Battery Charger Topologies, Charging Pow.pdf:application/pdf},
