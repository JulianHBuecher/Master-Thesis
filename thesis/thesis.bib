@article{garcia_sgam-based_2023,
	title = {{SGAM}-Based Analysis for the Capacity Optimization of Smart Grids Utilizing e-Mobility: The Use Case of Booking a Charge Session},
	volume = {16},
	issn = {1996-1073},
	url = {https://www.mdpi.com/1996-1073/16/5/2489},
